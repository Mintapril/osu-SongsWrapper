<Window x:Name="MainWindow1" x:Class="MapCollator.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MapCollator"
        mc:Ignorable="d"
        Title="MapCollator by mint" WindowStyle="ThreeDBorderWindow" Height="338.632" Width="591.488" HorizontalAlignment="Center" VerticalAlignment="Center" ResizeMode="NoResize">
	<Grid HorizontalAlignment="Left" Height="291" VerticalAlignment="Top" Width="589" Margin="0,0,0,-1.177">
		<Grid HorizontalAlignment="Left" Margin="117,25,0,0" Width="418" Height="235" VerticalAlignment="Top">
			<Grid Margin="68.36,61,-47.576,0" HorizontalAlignment="Center" VerticalAlignment="Top">
				<Label Content="Pack Name:" Width="100" Height="24" FontSize="14" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="-40,-20,0,-4"/>
				<TextBox x:Name="PackNameBox" Height="25" TextWrapping="Wrap" Width="237" FontSize="16" Text="Collection 1st" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="60,-20,0,-5"/>
			</Grid>
			<Grid Margin="68.36,97,-47.576,0" HorizontalAlignment="Center" VerticalAlignment="Top">
				<Label Content="Artists:" Width="100" Height="24" FontSize="14" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="-40,-19,0,-5"/>
				<TextBox x:Name="ArtistsBox" Height="25" TextWrapping="Wrap" Width="237" FontSize="16" Text="Various" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="60,-20,0,-5"/>
			</Grid>
			<Grid Margin="68.506,214,103.569,-58.912" HorizontalAlignment="Center" VerticalAlignment="Top">
				<Label Content="HP:" Width="100" Height="24" FontSize="14" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="-40,-19,0,-5"/>
				<TextBox x:Name="HPBox" Height="25" TextWrapping="Wrap" Width="86" FontSize="16" Text="7.5" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="60,-20,-0.177,-5"/>
			</Grid>
			<Grid Margin="68.506,176,103.569,-20.912" HorizontalAlignment="Center" VerticalAlignment="Top">
				<Label Content="OD:" Width="100" Height="24" FontSize="14" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="-40,-19,0,-5"/>
				<TextBox x:Name="ODBox" Height="25" TextWrapping="Wrap" Width="86" FontSize="16" Text="9" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="60,-20,-0.177,-5"/>
			</Grid>
			<Button x:Name="Start" Content="Start" Width="94" Height="45" Click="Start_Click" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="292,165,0,0"/>
			<Grid Margin="68.36,135,-47.576,0" HorizontalAlignment="Center" VerticalAlignment="Top">
				<Label Content="Creator:" Width="100" Height="24" FontSize="14" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="-40,-19,0,-5"/>
				<TextBox x:Name="CreatorBox" Height="25" TextWrapping="Wrap" Width="237" FontSize="16" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="60,-20,0,-5"/>
			</Grid>
		</Grid>
		<Grid HorizontalAlignment="Left" Height="48" Margin="88,6,0,0" VerticalAlignment="Top" Width="426">
			<TextBox x:Name="PathBox" Height="25" TextWrapping="Wrap" Width="310" FontSize="14" IsEnabled="False" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="10,11,0,0"/>
			<Button x:Name="Botton" Content="..." Width="81" Height="24" Click="Botton_Click" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="330,12,0,0"/>
		</Grid>
		<Grid HorizontalAlignment="Left" Height="227" Margin="39,28,0,0" VerticalAlignment="Top" Width="136">
			<ListView x:Name="ListView" HorizontalAlignment="Left" Height="185" Margin="10,42,0,0" VerticalAlignment="Top" Width="126" IsSynchronizedWithCurrentItem="False">
				<ListView.View>
					<GridView>
						<GridViewColumn/>
					</GridView>
				</ListView.View>
			</ListView>
		</Grid>
		<Label Content="Folder list" HorizontalAlignment="Left" Height="24" Margin="50,45,0,0" VerticalAlignment="Top" Width="68"/>
	</Grid>
</Window>